{
  "exe_dir": {
    "dir": "xml/tor1",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "teixml2html.py",
        "-i xml/tor1/$F.xml",
        "-o html/tor1/txt/$F.html",
        "-c cnf/tor1_dipl_txt.json",
        "-wa w "
      ],
      [
        "writehtml.py ",
        "-o html/tor1/txt/$F.html",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/tor1/$F.xml",
        "-o html/tor1/txt/$F.html",
        "-c cnf/tor1_inter_txt.json",
        "-wa a "
      ]
    ]
  },
  "remove_dir": [
    {
      "dir": "html/tor1/txt",
      "pattern": "tor1_list.html"
    }
  ]
}