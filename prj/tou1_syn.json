{
  "exe_dir": {
    "dir": "xml/tou1",
    "pattern": "*.xml",
    "par_subst": ".xml|",
    "par_name": "$F",
    "exe_file": [
      [
        "writehtml.py",
        "-o html/tou1/syn/$F.html",
        "-i '<div id=\"tou1_dipl_id\" class=\"text_pannel tei_dipl\">'",
        "-wa w"
      ],
      [
        "teixml2html.py",
        "-i xml/tou1/$F.xml",
        "-o html/tou1/syn/$F.html",
        "-c cnf/tou1_dipl_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/tou1/syn/$F.html",
        "-i '</div><div id=\"tou1_int_id\" class=\"text_pannel tei_int\">'",
        "-wa a"
      ],
      [
        "teixml2html.py",
        "-i xml/tou1/$F.xml",
        "-o html/tou1/syn/$F.html",
        "-c cnf/tou1_inter_syn.json",
        "-wa a"
      ],
      [
        "writehtml.py",
        "-o html/tou1/syn/$F.html",
        "-i '</div>'",
        "-wa a"
      ]
    ]
  },
  "remove_dir": [
    {
      "dir": "html/tou1/syn",
      "pattern": "tou1_list.html"
    }
  ]
}